Перем Описание Экспорт; //
Перем ПоУмолчанию Экспорт; //
Перем Значение Экспорт; //

Процедура ПриСозданииОбъекта(ПИмя, ПТип, ПОписание = "", ППоУмолчанию = Ложь, ПЗначение = Неопределено)
	
	Имя = ПИмя;
	Тип = ПТип;
	Описание = ПОписание;
	ПоУмолчанию = ?(ППоУмолчанию = Неопределено, Ложь, ППоУмолчанию);
	Значение = ПЗначение;

